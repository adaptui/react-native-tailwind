{
  "name": "react-native-system-example",
  "description": "Example app for react-native-system",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 12'",
    "start": "react-native start --reset-cache",
    "pods": "cd ios && pod install && cd ..",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "expo:android": "expo run:android",
    "expo:ios": "expo run:ios",
    "expo:start": "expo start",
    "expo:web": "expo start:web"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./App",
        "./App/stories"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.ts"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "expo": "^43.0.3",
    "react": "16.13.1",
    "react-native": "^0.64.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "^2.2.2",
    "react-native-svg": "^12.1.1",
    "tailwindcss": "^2.2.19",
    "twrnc": "^2.1.1",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-dom": "^17.0.2",
    "react-native-storybook-loader": "^2.0.4"
  }
}
