{
  "name": "@adaptui/react-native-tailwind",
  "version": "1.0.0-alpha.5",
  "description": "Collection of components that are accessible, composable, customizable from low level to build your own UI & Design System",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "homepage": "https://github.com/adaptui/react-native-tailwind.git#readme",
  "bugs": {
    "url": "https://github.com/adaptui/react-native-tailwind.git/issues"
  },
  "repository": "https://github.com/adaptui/react-native-tailwind.git",
  "license": "MIT",
  "author": "Timeless <hello@timeless.co>",
  "sideEffects": false,
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "source": "src/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-system.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "postinstall": "husky install",
    "bootstrap": "yarn example && yarn",
    "example": "yarn --cwd example",
    "types:check": "tsc --noEmit",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --color --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "prettier --check \"./**/*.{html,css,js,jsx,ts,tsx,md,json}\"",
    "format:fix": "prettier -wl \"./**/*.{html,css,js,jsx,ts,tsx,md,json}\"",
    "format:package": "sort-package-json package.json",
    "test": "jest",
    "prebuild": "rimraf lib",
    "build": "bob build",
    "prepublishOnly": "pinst --disable",
    "release": "release-it",
    "postpublish": "pinst --enable",
    "commit": "gacp",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,tx,tsx}": [
      "yarn lint:fix"
    ],
    "**/*.{html,css,js,jsx,ts,tsx,md,json}": [
      "yarn format:fix"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@chakra-ui/hooks": "^2.0.0",
    "@gorhom/bottom-sheet": "^4.3.1",
    "@react-aria/visually-hidden": "^3.3.0",
    "@react-native-aria/checkbox": "^0.2.3",
    "@react-native-aria/focus": "^0.2.5",
    "@react-native-aria/interactions": "^0.2.8",
    "@react-native-aria/radio": "^0.2.5",
    "@react-stately/toggle": "^3.3.0",
    "lodash": "^4.17.21",
    "react-native-gesture-handler": "2.1.0",
    "react-native-popper": "^0.3.2",
    "react-native-reanimated": "2.3.1",
    "react-native-svg": "^12.1.1",
    "tailwind-merge": "^1.2.1",
    "tailwindcss": "^3.0.24",
    "twrnc": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/eslint-parser": "7.18.2",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@react-native-community/eslint-config": "3.0.2",
    "@release-it/conventional-changelog": "5.0.0",
    "@types/jest": "28.1.1",
    "@types/react": "17.0.30",
    "@types/react-native": "0.64.24",
    "all-contributors-cli": "6.20.0",
    "commitlint": "17.0.2",
    "eslint": "8.17.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "gacp": "3.0.2",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "lint-staged": "13.0.1",
    "pinst": "3.0.0",
    "prettier": "2.6.2",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-builder-bob": "0.18.2",
    "release-it": "15.0.0",
    "sort-package-json": "1.57.0",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "react": "16.x || 17.x",
    "react-native": ">=0.62"
  },
  "resolutions": {
    "**/@types/react": "17.0.30"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gacp": {
    "add": false,
    "push": false,
    "emoji": "emoji",
    "editor": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
