{
  "name": "react-native-system",
  "version": "0.1.0",
  "description": "Collection of components that are accessible, composable, customizable from low level to build your own UI & Design System",
  "main": "node_modules/expo/AppEntry.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "react-native-system.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "lint": "eslint . \"**/*.{js,ts,tsx}\"",
    "lint-fix": "eslint . \"**/*.{js,ts,tsx}\" --fix",
    "format": "prettier --check **/*.tsx **/*.ts *.js",
    "format-fix": "prettier --write **/*.tsx **/*.ts *.js",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "node ./scripts/supressWarning.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/timelessco/react-native-system.git",
  "author": "Karthik-B-06 <hello@timeless.co> (https://timeless.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timelessco/react-native-system.git/issues"
  },
  "homepage": "https://github.com/timelessco/react-native-system.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.176",
    "@types/react-native": "^0.65.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-native-web": "^0.17.5",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "expo": "~44.0.5",
    "expo-status-bar": "~1.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.64.3",
    "react-native-builder-bob": "^0.18.1",
    "react-native-web": "^0.17.5",
    "release-it": "^14.2.2",
    "typescript": "^4.4.2",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "yarn": "^1.22.17"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@chakra-ui/hooks": "^1.7.1",
    "@react-native-aria/interactions": "^0.2.8",
    "@react-native-aria/overlays": "^0.3.2",
    "@gorhom/bottom-sheet": "^4",
    "@react-aria/visually-hidden": "^3.2.7",
    "@react-stately/toggle": "^3.2.5",
    "lodash": "^4.17.21",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-aria": "^0.2.3",
    "react-native-gesture-handler": "2.1.0",
    "react-native-popper": "^0.3.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-svg": "^12.1.1",
    "tailwind-merge": "^1.1.1",
    "tailwindcss": "^3.0.24",
    "twrnc": "^3.3.2"
  }
}
