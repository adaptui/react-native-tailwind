{
  "name": "@adaptui/react-native-tailwind",
  "version": "1.0.0-alpha.8",
  "description": "Collection of components that are accessible, composable, customizable from low level to build your own UI & Design System",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "homepage": "https://github.com/adaptui/react-native-tailwind.git#readme",
  "bugs": {
    "url": "https://github.com/adaptui/react-native-tailwind.git/issues"
  },
  "repository": "https://github.com/adaptui/react-native-tailwind.git",
  "license": "MIT",
  "author": "Timeless <hello@timeless.co>",
  "sideEffects": false,
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "source": "src/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-system.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "postinstall": "husky install",
    "bootstrap": "yarn example && yarn",
    "example": "yarn --cwd example",
    "types:check": "tsc --noEmit",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --color --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "prettier --check \"./**/*.{html,css,js,jsx,ts,tsx,md,json}\"",
    "format:fix": "prettier -wl \"./**/*.{html,css,js,jsx,ts,tsx,md,json}\"",
    "format:package": "sort-package-json package.json",
    "test": "jest",
    "prebuild": "rimraf lib",
    "build": "bob build",
    "prepublishOnly": "pinst --disable",
    "release": "release-it",
    "postpublish": "pinst --enable",
    "commit": "gacp",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "fix:dependencies": "yarn rnx-align-deps --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,tx,tsx}": [
      "yarn lint:fix"
    ],
    "**/*.{html,css,js,jsx,ts,tsx,md,json}": [
      "yarn format:fix"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@chakra-ui/hooks": "2.0.0",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@react-aria/focus": "^3.10.0",
    "@react-aria/visually-hidden": "^3.6.0",
    "@react-native-aria/checkbox": "^0.2.3",
    "@react-stately/toggle": "^3.3.0",
    "lodash": "^4.17.21",
    "react-native-popper": "^0.3.2",
    "tailwind-merge": "^1.8.0",
    "tailwindcss": "^3.2.4",
    "twrnc": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "7.18.9",
    "@babel/helper-string-parser": "^7.19.4",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.3.0",
    "@react-native-community/eslint-config": "3.0.3",
    "@release-it/conventional-changelog": "5.0.0",
    "@rnx-kit/align-deps": "^2.0.1",
    "@types/jest": "29.1.2",
    "@types/react": "18.0.0",
    "@types/react-native": "0.70.0",
    "all-contributors-cli": "6.20.0",
    "commitlint": "17.0.3",
    "eslint": "8.20.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "gacp": "3.0.2",
    "husky": "8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "13.0.3",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "react": "18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.70.0",
    "react-native-builder-bob": "0.18.3",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-svg": "^12.3.0",
    "release-it": "15.1.4",
    "sort-package-json": "2.0.0",
    "typescript": "4.8.3"
  },
  "peerDependencies": {
    "react": "16.9.0 || 16.11.0 || 16.13.1 || 17.0.1 || 17.0.2 || 18.0.0 || 18.1.0",
    "react-dom": "16.9.0 || 16.11.0 || 16.13.1 || 17.0.1 || 17.0.2 || ^18.0.0 || ^18.1.0",
    "react-native": "^0.61.5 || ^0.62.3 || ^0.63.2 || ^0.64.2 || ^0.65.0 || ^0.66.0 || ^0.67.0 || ^0.68.0 || ^0.69.0 || ^0.70.0",
    "react-native-gesture-handler": "^1.9.0 || ^1.10.3 || ^2.3.2 || ^2.5.0 || ^2.6.0",
    "react-native-reanimated": "^1.13.3 || ^2.1.0 || ^2.2.1 || ^2.2.3 || ^2.2.4 || ^2.5.0 || ^2.9.0 || ^2.10.0",
    "react-native-svg": "^12.1.1 || ^12.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gacp": {
    "add": false,
    "push": false,
    "emoji": "emoji",
    "editor": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "rnx-kit": {
    "kitType": "library",
    "alignDeps": {
      "requirements": {
        "development": [
          "react-native@0.70"
        ],
        "production": [
          "react-native@*"
        ]
      },
      "capabilities": [
        "animation",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "react",
        "react-dom",
        "svg"
      ]
    }
  }
}
